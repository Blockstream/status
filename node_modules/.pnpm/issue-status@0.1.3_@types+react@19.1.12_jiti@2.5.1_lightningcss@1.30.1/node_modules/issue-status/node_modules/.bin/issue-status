#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/chase/Sites/blockstream/status/node_modules/.pnpm/issue-status@0.1.3_@types+react@19.1.12_jiti@2.5.1_lightningcss@1.30.1/node_modules/issue-status/node_modules:/home/chase/Sites/blockstream/status/node_modules/.pnpm/issue-status@0.1.3_@types+react@19.1.12_jiti@2.5.1_lightningcss@1.30.1/node_modules:/home/chase/Sites/blockstream/status/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/chase/Sites/blockstream/status/node_modules/.pnpm/issue-status@0.1.3_@types+react@19.1.12_jiti@2.5.1_lightningcss@1.30.1/node_modules/issue-status/node_modules:/home/chase/Sites/blockstream/status/node_modules/.pnpm/issue-status@0.1.3_@types+react@19.1.12_jiti@2.5.1_lightningcss@1.30.1/node_modules:/home/chase/Sites/blockstream/status/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../index.js" "$@"
else
  exec node  "$basedir/../../index.js" "$@"
fi
